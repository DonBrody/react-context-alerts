{
  "env": {
    "cjs": {
      "presets": [["airbnb", { "removePropTypes": true }]],
      "plugins": [
        ["inline-react-svg", {
          "svgo": false
        }],
        ["transform-replace-object-assign", { "moduleSpecifier": "object.assign" }],
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
